{
  "compilerOptions": {
    "lib": ["es2019"],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./dist/nodes",
    "rootDir": "./nodes",
    "target": "es2019",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["nodes/**/*"],
  "exclude": ["node_modules/**/*", "dist/**/*"]
} 